          <div class="card-body p-4 p-md-5">

            <h3 class="mb-4 pb-2 pb-md-0 mb-md-5">Predictions</h3>
            <div class="row">
                <div class="col-md-3 ">
                 
                <h6>Disease</h6>

                </div>

                <div class="col-md-5 ">
           
                    <h6>Percentage Prediction</h6>

                   </div>
                   <div class="col-md-4  ">     
                    <h6>Learn More</h6>    
                   </div>
            </div>
            <% if(typeof diseases != 'undefined') { %>
            <hr size="4px">
                    <% if(diseases.Appendicitis != null) { %>
                    <div class="row">
                      <div class="col-md-3 mb-4">  
                        <h6>Appendicitis</h6>
                      </div>

                      <div class="col-md-3 mb-4 ">

                        <div class='percentageFill_appendicitis'><%= diseases.Appendicitis %>%</div>
                     </div>

                      <div class="col-md-3 mb-4 offset-md-3">
          
                         <button style="background-color: green;" name="View" type="button" class="btn btn-primary btn-sm"><a href = "https://www.nhs.uk/conditions/appendicitis/symptoms/" style=" color:white; text-decoration:none;">View</a></button>
                       </div>
                    </div>
                    <% } %>

                    <% if(diseases.Diabetes != null) { %>
                    <div class="row">
                        <div class="col-md-3 mb-4">  
                            <h6>Diabetes</h6>
                        </div>
  
                        <div class="col-md-3 mb-4 ">
  
                            <div class='percentageFill_diabetes'><%= diseases.Diabetes %>%</div>
                       </div>
  
                        <div class="col-md-3 mb-4 offset-md-3">
            
                           <button style="background-color: green;" name="View" type="button" class="btn btn-primary btn-sm"><a href = "https://www.diabetes.org.uk/diabetes-the-basics/diabetes-symptoms" style=" color:white; text-decoration:none;">View</a></button>
                         </div>
                      </div>
                      <% } %>
                      <% if(diseases.HeartAttack != null) { %>
                      <div class="row">
                        <div class="col-md-3 mb-4">  
                            <h6>Heart Attack</h6>
                        </div>
  
                        <div class="col-md-3 mb-4 ">
  
                            <div class='percentageFill_heartattack'><%= diseases.HeartAttack %>%</div>
                       </div>
  
                        <div class="col-md-3 mb-4 offset-md-3">
            
                           <button style="background-color: green;" name="View" type="button" class="btn btn-primary btn-sm"><a href = "https://www.nhs.uk/conditions/heart-attack/symptoms/" style=" color:white; text-decoration:none;">View</a></button>
                         </div>
                      </div>
                      <% } %>
                      <% if(diseases.Jaundice != null) { %>
                      <div class="row">
                        <div class="col-md-3 mb-4">  
                            <h6>Jaundice</h6>
                        </div>
  
                        <div class="col-md-3 mb-4 ">
  
                            <div class='percentageFill_jaundice'><%= diseases.Jaundice %>%</div>
                       </div>
  
                        <div class="col-md-3 mb-4 offset-md-3">
            
                           <button style="background-color: green;" name="View" type="button" class="btn btn-primary btn-sm"><a href = "https://www.nhs.uk/conditions/jaundice/" style=" color:white; text-decoration:none;">View</a></button>
                         </div>
                      </div>
                      <% } %>
                      <% if(diseases.Malaria != null) { %>
                      <div class="row">
                        <div class="col-md-3 mb-4">  
                            <h6>Malaria</h6>
                        </div>
  
                        <div class="col-md-3 mb-4 ">
  
                            <div class='percentageFill_malaria'><%= diseases.Malaria %>%</div>
                       </div>
  
                        <div class="col-md-3 mb-4 offset-md-3">
            
                           <button style="background-color: green;" name="View" type="button" class="btn btn-primary btn-sm"><a href = "https://www.nhs.uk/conditions/malaria/" style=" color:white; text-decoration:none;">View</a></button>
                         </div>
                      </div>
                      <% } %>
                      <% if(diseases.Tuberculosis != null) { %>
                      <div class="row">
                        <div class="col-md-3 mb-4">  
                            
                        <h6>Tuberculosis</h6>
                        </div>
  
                        <div class="col-md-3 mb-4 ">
  
                            <div class='percentageFill_tuberculosis'><%= diseases.Tuberculosis %>%</div>
                       </div>
  
                        <div class="col-md-3 mb-4 offset-md-3">
            
                           <button style="background-color: green;" name="View" type="button" class="btn btn-primary btn-sm"><a href = "https://www.nhs.uk/conditions/tuberculosis-tb/symptoms/" style=" color:white; text-decoration:none;">View</a></button>
                         </div>
                      </div>
                      <% } %>
              <% } else { %>
                 <p style="color: red">There are no predicted diseases</p>
              <% } %>
              &nbsp&nbsp&nbsp&nbsp &nbsp&nbsp&nbsp&nbsp
              <div class="mt-4 pt-2">
                <button name="Back" type="button" class="btn btn-primary btn-lg custom-btn" onclick="history.go(-1)">Back to Dashboard</button>
              </div>

            </form>
          </div>
